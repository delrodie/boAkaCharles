<div class="section mt-7 mb-9">
					<div class="container">
						<div class="row">
							{% for actualite in actualites %}
                            <div class="col-md-4">
								<div class="banner">
									<a href="#" class="link" titre="{{ actualite.titre }}">
										<img src="{{ vich_uploader_asset(actualite, 'imageFile') }}" alt="{{ actualite.titre }}" />
									</a>
									<div class="caption">
										<span>{{ actualite.publieLe|date('Y-m-d h:i:s')|duree}}</span>
									</div>
								</div>
							</div>  
                            {% endfor %}
						</div>
					</div>
				</div>