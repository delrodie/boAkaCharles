  <![endif]-->

</head>
<body class="hold-transition login-page" style="background: url(asset('backend/images/login-register.jpg')) center center no-repeat #d2d6de;">
<div class="login-box">
  <div class="login-logo" style="font-weight:bold;">
    <a href="#">AKA CHARLES</a>
  </div>
  <!-- /.login-logo -->
  {% block body %}{% endblock %}
  <!-- /.login-box-body -->
</div>
<!-- /.login-box -->

    {% javascripts 
        'backend/assets/vendor_components/jquery/dist/jquery.min.js'
        'backend/assets/vendor_components/popper/dist/popper.min.js'
        'backend/assets/vendor_components/bootstrap/dist/js/bootstrap.min.js'
        output='backend/js/applogin.js'
    %}
    <script src="{{ absolute_url(asset_url)}}"></script>
    {% endjavascripts %}

</body>

</html>
